				},
				{
					"lang": "cpp",
					"name": "C++"
				},
				{
					"lang": "c",
					"name": "C"
				},
				{
					"lang": "py",
					"name": "Python"
				},
				{
					"lang": "js",
					"name": "JavaScript"
				},
				{
					"lang": "scratch",
					"name": "Scratch"
				},
				{
					"lang": "hs",
					"name": "Haskell"
				},
				{
					"lang": "rs",
					"name": "Rust"
				},
				{
					"lang": "ml",
					"name": "OCaml"
				},
				{
					"lang": "java",
					"name": "Java"
				},
				{
					"lang": "clj",
					"name": "Clojure"
				},
				{
					"lang": "elm",
					"name": "Elm"
				},
				{
					"lang": "LabVIEW",
					"name": "LabVIEW"
				},
				{
					"lang": "d",
					"name": "D"
				},
				{
					"lang": "go",
					"name": "Go"
				},
				{
					"lang": "racket",
					"name": "Racket"
				},
				{
					"lang": "m",
					"name": "Matlab"
				}

			],
			"split": false
        }
	}
}
