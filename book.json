{
    "plugins": ["mathjax", "bibtex-cite", "creativecommons", "wordcount", "theme-api", "include-codeblock"],
    "lunr": {
        "maxIndexSize": 1000000000
    },
	"pluginsConfig": {
		"theme-api": {
			"languages": [
				{
					"lang": "pseudo",
					"name": "Pseudocode",
					"default": true
				},
				{
					"lang": "js",
					"name": "JavaScript"
				},
				{
					"lang": "scratch",
					"name": "SCRATCH"
				},
				{
					"lang": "py3",
					"name": "python 3"
				},
				{
					"lang": "py2",
					"name": "python 2"
				},
				{
					"lang": "c",
					"name": "C"
				},
				{
					"lang": "cpp",
					"name": "C++"
				},
				{
					"lang": "cs",
					"name": "C#"	
				}
			],
			"split": true 
		}
	}
}
