{
    "gitbook": "3.x.x",
    "plugins": ["mathjax", "bibtex-cite", "creativecommons", "wordcount", "theme-api", "include-codeblock", "ga"],
    "lunr": {
        "maxIndexSize": 1000000000
    },
	"pluginsConfig": {
		"include-codeblock": {
			"fixlang": true,
			"unindent": true
		},
                "ga": {
                     "token": "UA-118252470-1"
                 },
